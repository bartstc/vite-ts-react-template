{
  "name": "spa-vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:ci": "vitest --reporter junit --outputFile unit-report.xml",
    "preview": "vite preview",
    "lint": "CI=true eslint --ignore-pattern '*.d.ts' --ignore-pattern '*.test.*' --ext ts,tsx --max-warnings 0 src",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "storybook-latest-node": "export NODE_OPTIONS=--openssl-legacy-provider; storybook dev -p 6006",
    "test-storybook": "pnpm exec test-storybook --watch",
    "build-storybook": "storybook build",
    "preview-storybook": "http-server storybook-static --port 6006 --silent",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm build-storybook --quiet && pnpm preview-storybook\" \"wait-on tcp:127.0.0.1:6006 && pnpm exec test-storybook --json --outputFile storybook-report.json a\""
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-a11y": "^7.0.26",
    "@storybook/addon-actions": "^7.0.26",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/addon-mdx-gfm": "^7.0.26",
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "^7.0.26",
    "@storybook/react-vite": "^7.0.26",
    "@storybook/test-runner": "^0.10.0",
    "@storybook/testing-library": "^0.2.0",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react-swc": "^3.1.0",
    "axe-playwright": "^1.2.3",
    "babel-loader": "^8.3.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.27.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-storybook": "^0.6.12",
    "http-server": "^14.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "msw": "^1.1.0",
    "msw-storybook-addon": "^1.8.0",
    "playwright": "^1.35.1",
    "prettier": "2.8.6",
    "pretty-quick": "^3.1.3",
    "start-server-and-test": "^2.0.0",
    "storybook": "^7.0.26",
    "storybook-dark-mode": "^3.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.4",
    "vite-plugin-checker": "^0.5.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.27.3",
    "wait-on": "^7.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "last 2 versions"
    ],
    "development": [
      "last 1 version"
    ]
  },
  "packageManager": "pnpm@8.6.6"
}
