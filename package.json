{
  "name": "spa-vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:ci": "vitest run && vitest --reporter junit --outputFile unit-report.xml",
    "preview": "vite preview",
    "lint": "CI=true eslint --ignore-pattern '*.d.ts' --ignore-pattern '*.test.*' --ext ts,tsx --max-warnings 0 src",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "pnpm exec test-storybook --watch",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@storybook/addon-a11y": "^7.5.1",
    "@storybook/addon-actions": "^7.5.1",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/addon-mdx-gfm": "^7.5.1",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.5.1",
    "@storybook/react-vite": "^7.5.1",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.2",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "axe-playwright": "^1.2.3",
    "babel-loader": "^8.3.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.27.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-storybook": "^0.6.15",
    "http-server": "^14.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "msw": "^1.1.0",
    "msw-storybook-addon": "^1.9.0",
    "playwright": "^1.41.1",
    "prettier": "2.8.6",
    "pretty-quick": "^3.1.3",
    "storybook": "^7.5.1",
    "storybook-dark-mode": "^3.0.1",
    "typescript": "^5.3.3",
    "vite": "~5.0.12",
    "vite-plugin-checker": "^0.6.2",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.1",
    "wait-on": "^7.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "last 2 versions"
    ],
    "development": [
      "last 1 version"
    ]
  },
  "packageManager": "pnpm@8.14.1"
}
